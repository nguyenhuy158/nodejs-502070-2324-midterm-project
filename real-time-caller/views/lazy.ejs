    <script type="text/javascript"
            src="https://cdnjs.cloudflare.com/ajax/libs/jquery.lazy/1.7.9/jquery.lazy.plugins.min.js"></script>

    <style>
        img.lazy {
            width: 700px;
            height: 467px;
            display: block;
        }
    </style>
</head>
<body>
<img class="lazy"
